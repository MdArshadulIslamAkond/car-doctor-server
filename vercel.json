{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "redirects": [
        { "source": "/(.*)", "destination": "/index.js" }
      ],
    "routes":[
        
       { 
        "src": "/(.*)",
        "dest": "/",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"] 
        }
    ]

}
       